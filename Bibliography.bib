@ARTICLE{agm99:493,
author = {Noam Agmon},
title = {Proton solvation and proton mobility},
journal = {Isr. J. Chem.},
year = {1999},
volume = {39},
pages = {493 -- 502},
abstract = {Experimental evidence for proton solvation and proton mobility is
    analyzed and the results are compared with recent simulations. Three
    factors contribute to differences in proton solvation energies: hydrogen-bond
    cleavage, changes in hydrogen-bond lengths, and proton delocalization.
    These factors are estimated from experimental attributes. In dilute
    acidic aqueous solutions H3O+ is more stable than H5O2+ by about
    0.6 kcal/mol. This estimate, together with the activation energy
    for proton mobility, supports the 121 mechanism for proton mobility
    in which a protonated water monomer is transformed, by second-shell
    hydrogen-bond cleavage, to a protonated dimer and back to another
    protonated monomer.}
}

@ARTICLE{agm05:2452,
author = {N. Agmon},
title = {Proton pathways in green fluorescence protein},
journal = {Biophys. J.},
year = {2005},
volume = {88},
pages = {2452 -- 2461},
abstract = {Proton pathways in green fluorescent protein (GFP) are more extended
    than previously reported. In the x-ray data of wild-type GFP, a two-step
    exit pathway exists from the active site to the protein surface,
    controlled by a threonine switch. A proton entry pathway begins at
    a glutamate-lysine cluster around Glu-5, and extends all the way
    to the buried Glu-222 near the active site. This structural evidence
    suggests that GFP may function as a portable light-driven proton-pump,
    with proton emitted in the excited state through the switchable exit
    pathway, and replenished from Glu-222 and the Glu-5 entry pathway
    in the ground state.},
doi = {10.1529/biophysj.104.055541},
keywords = {EXCITED-STATE DYNAMICS; STRUCTURAL BASIS; SPECTRAL VARIATIONS; GFP;}
}

